# Ticketmaster API
TICKETMASTER_API_KEY=your_ticketmaster_api_key

# Supabase Postgres direct connection (find in Supabase Dashboard → Settings → Database)
DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# System identity UUIDs (for rows written by this service)
SYSTEM_BUSINESS_ID=00000000-0000-0000-0000-000000000000
SYSTEM_USER_ID=00000000-0000-0000-0000-000000000000

# Comma-separated cities to fetch (default: Cape Town)
CITIES=Cape Town,Johannesburg,Durban

# Page size for Ticketmaster API (20-200, default: 100)
PAGE_SIZE=100

# Run an immediate ingest on startup (default: false)
RUN_ON_START=true
