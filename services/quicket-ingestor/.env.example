# Quicket API
QUICKET_API_KEY=your_quicket_api_key

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJxxx

# System identity UUIDs (for rows written by this service)
SYSTEM_BUSINESS_ID=00000000-0000-0000-0000-000000000000
SYSTEM_USER_ID=00000000-0000-0000-0000-000000000000

# Comma-separated cities to fetch (default: Cape Town)
CITIES=Cape Town,Johannesburg,Durban

# Page size for Quicket API (default: 100)
PAGE_SIZE=100

# Run an immediate ingest on startup (default: false)
RUN_ON_START=true
