{
  "name": "sayso",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "next dev --turbo --port 3000",
    "dev:perf": "node scripts/dev-performance.js && npm run dev:fast",
    "build": "node scripts/run-build-with-polyfill.js",
    "build:analyze": "ANALYZE=true next build",
    "build:fast": "NODE_ENV=development next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs = require('fs'); ['.next', 'out', 'dist'].forEach(dir => { try { fs.rmSync(dir, { recursive: true, force: true }); } catch(e) {} });\"",
    "dev:clean": "npm run clean && npm run dev",
    "perf:check": "node scripts/dev-performance.js",
    "perf:audit": "node scripts/performance-audit.js",
    "fetch-events": "node scripts/fetch-events.js"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.12",
    "isomorphic-dompurify": "^2.33.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-feather": "^2.0.10",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.3",
    "webpack": "^5.102.1"
  },
  "engines": {
    "node": "20.x"
  }
