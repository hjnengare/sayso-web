{
  "name": "sayso",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "next dev --turbo --port 3000",
    "dev:perf": "node scripts/dev-performance.js && npm run dev:fast",
    "build": "node scripts/run-build-with-polyfill.js",
    "build:analyze": "ANALYZE=true next build",
    "build:fast": "NODE_ENV=development next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs = require('fs'); ['.next', 'out', 'dist'].forEach(dir => { try { fs.rmSync(dir, { recursive: true, force: true }); } catch(e) {} });\"",
    "dev:clean": "npm run clean && npm run dev",
    "perf:check": "node scripts/dev-performance.js",
    "perf:audit": "node scripts/performance-audit.js",
    "fetch-events": "node scripts/fetch-events.js",
    "seed:fsq": "tsx scripts/seed-foursquare-businesses.ts",
    "seed:osm": "tsx scripts/seed-osm-businesses.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:api": "jest --config=jest.api.config.js",
    "test:coverage": "jest --coverage",
    "test:vitest": "vitest",
    "test:vitest:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/mapbox-gl": "^3.4.1",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.12",
    "isomorphic-dompurify": "^2.33.0",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^3.17.0",
    "next": "^16.0.7",
    "openai": "^4.104.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-icons": "^5.5.0",
    "recharts": "^3.7.0",
    "resend": "^6.5.2",
    "supabase": "^2.65.6",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.7",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "undici": "^7.16.0",
    "vitest": "^4.0.18",
    "webpack": "^5.102.1",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": "20.x"
  }
}
