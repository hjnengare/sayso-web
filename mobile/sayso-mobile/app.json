{
  "expo": {
    "name": "Sayso",
    "slug": "sayso-mobile",
    "scheme": "sayso",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sayso.mobile"
    },
    "android": {
      "package": "com.sayso.mobile"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "${EXPO_PUBLIC_EAS_PROJECT_ID}"
      }
    }
  }
}
